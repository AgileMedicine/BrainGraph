<UserControl
	x:Class="BrainGraph.WinStore.Screens.Config.PermutationPopup"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="using:BrainGraph.WinStore.Screens.Selection"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	Height="82"
	Width="250">
	
	<Grid Margin="8">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition />
		</Grid.RowDefinitions>
		<TextBlock Text="Permutations" Grid.Row="0" Foreground="Black" Margin="0,0,0,8" FontSize="18.667" />
		<TextBox HorizontalAlignment="Stretch" VerticalAlignment="Top" Grid.Row="1" Text="{Binding Permutations, Mode=TwoWay}" />
	</Grid>
</UserControl>
